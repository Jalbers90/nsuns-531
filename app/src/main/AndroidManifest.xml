<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.jalbers.NSuns531">

    <uses-permission android:name="android.permission.INTERNET" />

    <application

        android:allowBackup="true"
        android:icon="@mipmap/nsuns_icon_bignblue"
        android:label="N Suns 5/3/1"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">


        <activity
            android:name="com.jalbers.NSuns531.MainActivity"
            android:theme="@style/Theme.AppCompat.NoActionBar"
            android:windowSoftInputMode="adjustNothing">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>

        <activity
            android:name="com.jalbers.NSuns531.WeekOverviewActivity"
            android:theme="@style/Theme.AppCompat" />

        <activity
            android:name="com.jalbers.NSuns531.SettingsActivity"
            android:theme="@style/Theme.AppCompat">
        </activity>

    </application>

</manifest>